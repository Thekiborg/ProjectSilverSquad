<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <ThingDef ParentName="ResourceBase">
        <defName>SilverSquad_GenomeImprint</defName>
        <label>genome imprint</label>
        <thingClass>ProjectSilverSquad.ThingClass_GenomeImprint</thingClass>
        <description>A record of someone's body at a given point; it contains all the data needed to artificially grow a new body in the appropriate cloning vat.</description>
        <graphicData>
            <texPath>Things/Item/Resource/PlantFoodRaw/Potatoes</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <statBases>
            <MarketValue>400</MarketValue>
            <MaxHitPoints>60</MaxHitPoints>
            <Flammability>1.0</Flammability>
            <DeteriorationRate>6</DeteriorationRate>
            <Mass>0.03</Mass>
        </statBases>
        <thingCategories>
            <li>ItemsMisc</li>
        </thingCategories>
        <allowedArchonexusCount>5</allowedArchonexusCount>
    </ThingDef>
    
</Defs>