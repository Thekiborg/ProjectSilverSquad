<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <ThingDef ParentName="ResourceBase">
        <defName>SilverSquad_GenomeImprint</defName>
        <label>genome imprint</label>
        <thingClass>ProjectSilverSquad.ThingClass_GenomeImprint</thingClass>
        <description>A record of someone's body at a given point; it contains all the data needed to artificially grow a new body in the appropriate cloning vat.</description>
        <graphicData>
            <texPath>Things/Item/Resource/PlantFoodRaw/Potatoes</texPath>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <statBases>
            <MarketValue>550</MarketValue>
            <MaxHitPoints>200</MaxHitPoints>
            <Flammability>1.0</Flammability>
            <DeteriorationRate>2</DeteriorationRate>
            <Mass>0.1</Mass>
        </statBases>
        <costList>
            <Silver>50</Silver>
            <Plasteel>25</Plasteel>
            <ComponentIndustrial>2</ComponentIndustrial>
        </costList>
        <recipeMaker>
            <skillRequirements>
                <Crafting>8</Crafting>
            </skillRequirements>
            <researchPrerequisite>SilverSquad_CloningTech</researchPrerequisite>
            <recipeUsers>
                <li>FabricationBench</li>
            </recipeUsers>
        </recipeMaker>
        <thingCategories>
            <li>ItemsMisc</li>
        </thingCategories>
        <allowedArchonexusCount>5</allowedArchonexusCount>
    </ThingDef>
    
</Defs>