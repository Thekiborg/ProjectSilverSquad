<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <ProjectSilverSquad.BrainChipDef Name="ChipBase" ParentName="ResourceBase" Abstract="True">
        <graphicData>
            <graphicClass>Graphic_Single</graphicClass>
        </graphicData>
        <statBases>
            <MarketValue>400</MarketValue>
            <MaxHitPoints>100</MaxHitPoints>
            <Flammability>1.0</Flammability>
            <DeteriorationRate>2</DeteriorationRate>
            <Mass>0.03</Mass>
        </statBases>
        <thingCategories>
            <li>ItemsMisc</li>
        </thingCategories>
        <recipeMaker>
            <skillRequirements>
                <Crafting>5</Crafting>
            </skillRequirements>
            <researchPrerequisite>SilverSquad_CloningTech</researchPrerequisite>
            <recipeUsers>
                <li>FabricationBench</li>
            </recipeUsers>
        </recipeMaker>
        <allowedArchonexusCount>5</allowedArchonexusCount>
    </ProjectSilverSquad.BrainChipDef>

    <ProjectSilverSquad.BrainChipDef ParentName="ChipBase">
        <defName>SilverSquad_BrainChip_RawOffset_Shooting</defName>
        <label>Brain chip (Shooting)</label>
        <description>A brain chip automatically used when growing clones. It will increase the clone's shooting skill.</description>
        <graphicData>
            <texPath>Things/Item/Resource/PlantFoodRaw/Potatoes</texPath>
        </graphicData>
        <skillMods>
            <Shooting>5</Shooting>
        </skillMods>
        <instabilityOffset>0.1</instabilityOffset>
    </ProjectSilverSquad.BrainChipDef>

