<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <ThingDef ParentName="TableGatherSpotBase">
        <defName>SilverSquad_CloningVat</defName>
        <label>cloning vat</label>
        <thingClass>ProjectSilverSquad.ThingClass_CloningVat</thingClass>
        <tickerType>Rare</tickerType>
        <drawerType>MapMeshAndRealTime</drawerType>
        <graphicData>
            <texPath>CloningVat</texPath>
            <graphicClass>Graphic_Multi</graphicClass>
            <drawSize>(2,2)</drawSize>
            <damageData>
                <cornerTL>Damage/Corner</cornerTL>
                <cornerTR>Damage/Corner</cornerTR>
                <cornerBL>Damage/Corner</cornerBL>
                <cornerBR>Damage/Corner</cornerBR>
            </damageData>
        </graphicData>
        <uiIconScale>0.8</uiIconScale>
        <statBases>
            <MaxHitPoints>100</MaxHitPoints>
            <WorkToBuild>1500</WorkToBuild>
            <Mass>10</Mass>
            <Flammability>1.0</Flammability>
            <Beauty>1</Beauty>
            <StyleDominance MayRequire="Ludeon.RimWorld.Ideology">10</StyleDominance>
        </statBases>
        <size>(2,2)</size>
        <costStuffCount>50</costStuffCount>
        <rotatable>true</rotatable>
        <comps>
            <li Class="CompProperties_Power">
                <compClass>CompPowerTrader</compClass>
                <basePowerConsumption>900</basePowerConsumption>
                <idlePowerDraw>100</idlePowerDraw>
            </li>
        </comps>
        <modExtensions>
            <li Class="ProjectSilverSquad.ModExtension">
                <maxNutPasteCapacity>7</maxNutPasteCapacity>
                <baseNutConsumptionPerDay>1</baseNutConsumptionPerDay>
                <basePawnGrowTimeTicks>720,000</basePawnGrowTimeTicks>
                <baseEmbryoIncubationTicks>180,000</baseEmbryoIncubationTicks>
                <ticksOfNoReturn>360,000</ticksOfNoReturn>
                <embryoGraphicData>
                    <texPath>Things/Item/Resource/PlantFoodRaw/Potatoes</texPath>
                    <graphicClass>Graphic_Single</graphicClass>
                    <drawSize>(1,1)</drawSize>
                </embryoGraphicData>
            </li>
        </modExtensions>
    </ThingDef>

</Defs>