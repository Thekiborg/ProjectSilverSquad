<?xml version="1.0" encoding="UTF-8"?>
<Defs>

    <WorkGiverDef>
        <defName>SilverSquad_CarryIngredientsToVat</defName>
        <label>carry ingredients to cloning vat</label>
        <giverClass>ProjectSilverSquad.WorkGiver_CarryIngredientsToVat</giverClass>
        <workType>Hauling</workType>
        <verb>carry</verb>
        <gerund>carrying</gerund>
        <priorityInType>90</priorityInType>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
    </WorkGiverDef>

    <WorkGiverDef>
        <defName>SilverSquad_RecordGenome</defName>
        <label>scan someone's genome</label>
        <giverClass>ProjectSilverSquad.WorkGiver_RecordGenome</giverClass>
        <workType>Hauling</workType>
        <verb>scan</verb>
        <gerund>scanning</gerund>
        <priorityInType>90</priorityInType>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
    </WorkGiverDef>

    <WorkGiverDef>
        <defName>SilverSquad_LoadPasteIntoVat</defName>
        <label>loading paste into cloning vat</label>
        <giverClass>ProjectSilverSquad.WorkGiver_LoadPasteIntoVat</giverClass>
        <workType>Hauling</workType>
        <verb>load</verb>
        <gerund>loading</gerund>
        <priorityInType>90</priorityInType>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
        <modExtensions>
            <li Class="ProjectSilverSquad.ModExtension">
                <loadNutPasteThreshold>3</loadNutPasteThreshold>
            </li>
        </modExtensions>
    </WorkGiverDef>

    <WorkGiverDef>
        <defName>SilverSquad_EmptyVat</defName>
        <label>taking clone out of cloning vat</label>
        <giverClass>ProjectSilverSquad.WorkGiver_EmptyVat</giverClass>
        <workType>Hauling</workType>
        <verb>haul</verb>
        <gerund>hauling</gerund>
        <priorityInType>90</priorityInType>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
        <modExtensions>
            <li Class="ProjectSilverSquad.ModExtension">
                <loadNutPasteThreshold>3</loadNutPasteThreshold>
            </li>
        </modExtensions>
    </WorkGiverDef>

    <WorkGiverDef>
        <defName>SilverSquad_CraftChips</defName>
        <label>craft chips</label>
        <giverClass>WorkGiver_DoBill</giverClass>
        <workType>Crafting</workType>
        <priorityInType>40</priorityInType>
        <fixedBillGiverDefs>
            <li>SilverSquad_ChipWorkbench</li>
        </fixedBillGiverDefs>
        <verb>craft at</verb>
        <gerund>crafting at</gerund>
        <requiredCapacities>
            <li>Manipulation</li>
        </requiredCapacities>
        <prioritizeSustains>true</prioritizeSustains>
    </WorkGiverDef>
</Defs>